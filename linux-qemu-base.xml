<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Generic manifests for linux and qemu /!-->
  <remote  name="origin"
           fetch="https://github.com/" />

  <default revision="master"
           remote="origin"
           sync-j="2" />

  <!-- Specific projects /!-->
  <project path="linux"   name="torvalds/linux" />
  <project path="qemu"        name="qemu/qemu" />
  <project path="busybox"   name="mirror/busybox" />
  <project path="scripts" name="degilnpsvy/platform-scripts" >
    <linkfile src="Makefile" dest="Makefile"/>
  </project>
  <project path="module"  name="degilnpsvy/linux-modules" />

</manifest>
